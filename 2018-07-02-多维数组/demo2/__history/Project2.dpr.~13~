program Project2;

{$APPTYPE CONSOLE}

{$R *.res}

uses
    System.SysUtils;

var
    NumberArr: array of Integer;

var
    ax, bx, bt: Integer;

var
    I, J: Integer;

procedure Demo1();
begin
    ax := 10;
    bx := 30;

    //变量值的互换
    bt := ax;
    ax := bx;
    bx := bt;
end;

begin
    NumberArr := [30, 2, 1, 5];
    //排序
    for I := Low(NumberArr) to High(NumberArr) do begin
        ax := NumberArr[I];
        for J := Low(NumberArr) to High(NumberArr) do begin

            bx := NumberArr[J];
            if (ax > bx) then begin

            end;
        end;

    end;

    Writeln(ax, ',', bx);
    Readln;

end.

