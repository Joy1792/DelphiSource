program Project1;

uses
  Vcl.Forms,
  Unit1 in 'Unit1.pas' {MainForm},
  Vcl.Themes,
  Vcl.Styles,
  Unit2 in 'Unit2.pas',
  AddForm in 'AddForm.pas' {Form3};

